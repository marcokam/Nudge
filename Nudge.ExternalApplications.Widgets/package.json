{
  "name": "nudge.externalapplications.widgets",
  "version": "1.0.0",
  "description": "Nudge External Widgets",
  "main": "main.js",
  "scripts": {
    "ba": "npm run clean && npm run build && npm run analyze:widget",
    "analyze:widget": "source-map-explorer ../Nudge.ExternalApplications.Web/wwwroot/widgets/static/widget.js",
    "analyze:vendor": "source-map-explorer ../Nudge.ExternalApplications.Web/wwwroot/widgets/static/vendor~widget.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ../Nudge.ExternalApplications.Web/wwwroot/widgets/static",
    "build": "set NODE_ENV=production && webpack --mode=production --env.mode=analysis",
    "watch": "webpack --mode=development --watch",
    "start": "npm run clean&&webpack-dev-server --mode=development --colors --host integrations.dev.nudge.ai --https --history-api-fallback --hot --inline --compress"
  },
